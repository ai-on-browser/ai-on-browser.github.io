{
  "name": "@ai-on-browser/data-analysis-models",
  "version": "0.0.21",
  "description": "Data analysis model package without any dependencies",
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "minify": "find . -name \\*.js | grep -E '^./(lib|js)' | xargs -t -P2 -n1 -I% npx terser % -c -m -o %",
    "test": "yarn create-entry && NODE_OPTIONS=--experimental-vm-modules npx jest --coverage --reporters default ./tests/custom-reporter.js --maxWorkers=100%",
    "create-entry": "node ./create_import_list.js",
    "document": "yarn create-entry && documentation build lib -f html --github -o docs",
    "types": "yarn create-entry && find lib -name \\*.d.ts | xargs --no-run-if-empty rm && tsc lib/**.js lib/**/*.js --declaration --allowJs --emitDeclarationOnly",
    "build:gh-pages": "yarn document && sed -i -e /^doc/d .gitignore && yarn minify",
    "build:npm": "yarn create-entry"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ai-on-browser/ai-on-browser.github.io.git"
  },
  "keywords": [
    "Matrix",
    "AI",
    "Machine learning",
    "Data analysis",
    "Clustering",
    "Classification",
    "Regression",
    "Smoothing",
    "Anomaly detection",
    "Dimensionality reduction",
    "Reinforcement learning",
    "Neural network"
  ],
  "author": {
    "name": "Ishii Norimi",
    "url": "https://twitter.com/mirasunimoni"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ai-on-browser/ai-on-browser.github.io/issues"
  },
  "homepage": "https://ai-on-browser.github.io/docs",
  "devDependencies": {
    "documentation": "^13.2.5",
    "jest": "^27.2.0",
    "terser": "^5.9.0",
    "typescript": "^4.5.2"
  }
}
