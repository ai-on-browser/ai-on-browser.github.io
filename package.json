{
  "name": "@ai-on-browser/data-analysis-models",
  "version": "0.0.8",
  "description": "Data analysis models written completely in Javascript",
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "minify": "find . -name \\*.js | grep -E '^./(lib|js)' | xargs -I% npx terser -c -m -o % %",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "document": "documentation build lib -f html -o docs",
    "document:publish": "yarn document && sed -i -e /^doc/d .gitignore",
    "build": "node ./create_import_list.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ai-on-browser/ai-on-browser.github.io.git"
  },
  "keywords": [
    "JavaScript",
    "AI",
    "Machine learning",
    "Data analysis",
    "Neural network"
  ],
  "author": {
    "name": "Ishii Norimi",
    "url": "https://twitter.com/mirasunimoni"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ai-on-browser/ai-on-browser.github.io/issues"
  },
  "homepage": "https://github.com/ai-on-browser/ai-on-browser.github.io#readme",
  "devDependencies": {
    "documentation": "^13.2.5",
    "jest": "^27.2.0",
    "terser": "^5.9.0"
  }
}
